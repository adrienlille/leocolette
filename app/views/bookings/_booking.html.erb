<tr class="rowItem">
  <td>
    <ul class="list-inline listingsInfo">
      <li><a href="#"><img src="<%= booking.apartment.photo %>" alt="Image Listings"></a></li>
      <li>
        <h3><%= booking.apartment.name %> <i class="fa fa-check-circle" aria-hidden="true"></i></h3>
        <h5>1569 Queen Street West <span class="cityName">Toronto</span></h5>
        <span class="category">Hotel</span>
        <p>From $50.00 /Night <span class="likeArea"><i class="fa fa-heart-o" aria-hidden="true"></i>10k</span></p>
      </li>
    </ul>
  </td>
  <td><%= link_to "send message", conversations_path(user1_id: booking.apartment.user.id, user2_id: booking.user.id, apartment_id: booking.apartment_id), method: "post", class: "btn btn-primary" %></td>
  <td>784</td>
  <td>
    <ul class="list-inline rating">
      <li><i class="fa fa-star" aria-hidden="true"></i></li>
      <li><i class="fa fa-star" aria-hidden="true"></i></li>
      <li><i class="fa fa-star" aria-hidden="true"></i></li>
      <li><i class="fa fa-star" aria-hidden="true"></i></li>
      <li><i class="fa fa-star-o" aria-hidden="true"></i></li>
      <li>(7)</li>
    </ul>

  </td>
  <td><%= booking.start_date %></td>
  <td><%= booking.end_date %></td>
  <td><%= raw(booking.status_tag) %></td>
</tr>
